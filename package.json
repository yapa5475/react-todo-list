{
  "name": "react-todo-list",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://yapa5475.github.io/react-todo-list",
  "description": "A simple React Todo List application",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run vite\"",
    "vite": "vite",
    "server": "json-server --watch db.json --port 3001",
    "build": "vite build",
    "test": "jest --coverage",
    "debug-test": "node --inspect-brk ./node_modules/vitest/vitest.mjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^0.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-jest": "^30.0.2",
    "concurrently": "^9.2.0",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
